{
  "parser": "babel-eslint",
  "rules": {
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "strict": 0,
    "curly": 0,
    "quotes": [1, "single"],
    "space-infix-ops": 0,
    "eol-last": 0
  },
  "globals": {
    "requestAnimationFrame": true
  },
  "env": {
    "browser": true
  }
}